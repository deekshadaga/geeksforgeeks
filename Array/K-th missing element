int num = -1;
        for(int i=0; k>0&&i<arr.size()-1; i++){
            int diff = arr[i+1] - arr[i];
            if(diff>1){
                num = arr[i];
                while(k>0&&diff>1){
                    num ++;
                    k--;
                    diff--;
                
                }
            }
            
        }
        if(k!=0){
            return -1;
        }
        return num;
