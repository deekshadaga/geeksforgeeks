class Solution{
	public:
	int minCoins(int coins[], int n, int sum) 
	{ 
	    int t[n+1][sum+1];
	    for(int i=0;i<=n;i++){
	        for(int j=0;j<=sum;j++){
	            if(i==0) t[i][j]=INT_MAX-1;
	            if(j==0) t[i][j]=0;
	            if(i==1){
	                if(j%coins[0]==0) t[i][j]=j/coins[0];
	                else t[i][j]=INT_MAX-1;
	            }
	        }
	    }
	    for(int i=2;i<=n;i++){
	        for(int j=1;j<=sum;j++){
	            if(coins[i-1]<=j){
	                t[i][j]=min(t[i-1][j],1+t[i][j-coins[i-1]]);
	            }
	            else{
	                t[i][j]=t[i-1][j];
	            }
	        }
	    }
	    if(t[n][sum]!=INT_MAX-1) return t[n][sum];
	    return -1;
	} 
};
