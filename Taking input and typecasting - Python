def inPut():
    #Take input and assign the input to a,b,c,d. Please also typecast as type() will produce wrong answer without it
    a= int(input())## a is integer
    b= input()## b is string
    c= float(input())## c is float
    d= bool(input())## d is a boolean
    
    print(a, type(a))
    print(b, type(b))
    print(c, type(c))
    print(d, type(d))
