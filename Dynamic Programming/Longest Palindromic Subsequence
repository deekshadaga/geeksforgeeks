int longestPalinSubseq (string S)
{
    string a=S;
    reverse(S.begin(),S.end());
    string b=S;
    int n=S.length();
    int t[n+1][n+1];
    for(int i=0;i<=n;i++){
        for(int j=0;j<=n;j++){
            if(i==0 || j==0) 
                t[i][j] = 0;
            else if(a[i-1]==b[j-1]) 
                t[i][j] = 1+t[i-1][j-1];
            else
                t[i][j] = max(t[i-1][j],t[i][j-1]);
        }
    }
    return t[n][n];
}
